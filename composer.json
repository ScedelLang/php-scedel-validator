{
  "name": "scedel/validator",
  "description": "JSON validator for SCEDel schema repositories",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SCEDel contributors"
    }
  ],
  "keywords": [
    "scedel",
    "schema",
    "validator",
    "json"
  ],
  "require": {
    "php": "^8.3",
    "scedel/schema": "*"
  },
  "autoload": {
    "psr-4": {
      "Scedel\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scedel\\Validator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml"
  }
}
